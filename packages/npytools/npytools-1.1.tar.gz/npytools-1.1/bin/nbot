f = FB()

print("
 ---- [LOGIN - OPTONS] ---- 
 1) Login With FB Cookie In Json File
 2) Login By Entering Cookie Parameters
 
 ")
opt = str(input("OPTION : "))
if opt == '1':
 f.setcookie(input("Enter Full Path To Cookie File : "))
if opt == '2':
 f.cookieinput()
 
print("Logged In as : {}".format(f.getname()))
b = BOT(f)
b.InboxMathBOT()
