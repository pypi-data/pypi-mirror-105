{
  "name": "@epi2melabs/epi2melabs-splashpage",
  "version": "1.0.0",
  "description": "Accompanies the EPI2ME labs experience. Not for general use.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/epi2me-labs/epi2melabs-splashpage",
  "bugs": {
    "url": "https://github.com/epi2me-labs/epi2melabs-splashpage"
  },
  "license": "BSD-3-Clause",
  "author": "trich",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/**/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/epi2me-labs/epi2melabs-splashpage"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf epi2melabs_splashpage/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.10",
    "@jupyterlab/cells": "^3.0.10",
    "@jupyterlab/docmanager": "^3.0.10",
    "@jupyterlab/launcher": "^3.0.8",
    "@jupyterlab/notebook": "^3.0.10",
    "@jupyterlab/settingregistry": "^3.0.5",
    "@jupyterlab/ui-components": "^3.0.7",
    "@lumino/widgets": "^1.16.1",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "outputDir": "epi2melabs_splashpage/labextension"
  },
  "publishConfig": {
    "access": "public"
  },
  "styleModule": "style/index.js"
}